<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>DB_BankList</servlet-name>
    <servlet-class>net.proselyte.servletstutorial.DB_BankList</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Fields</servlet-name>
    <servlet-class>net.proselyte.servletstutorial.Fields</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>DB_BankList</servlet-name>
    <url-pattern>/DB_BankList</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Fields</servlet-name>
    <url-pattern></url-pattern>
  </servlet-mapping>
</web-app>